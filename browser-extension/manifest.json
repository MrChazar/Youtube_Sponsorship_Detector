{
    "manifest_version": 3,
    "name": "Youtube_Sponsorship_Detector",
    "version": "1.0.0",
    "description": "Youtube extension for getting rid of ads",
    "icons": {
        "32": "icons/icon32.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*//"
    ]
  }